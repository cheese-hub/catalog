{
    "key": "heartbleedhacker",
    "label": "HeartBleed Hacker",
    "description": "Demonstration of the HeartBleed bug in the OpenSSL library",
    "logo": "/asset/png/logos/ndslabs-badge.png",
    "image": {
        "name": "rkalyana/heartbleed-hacker"
    },
    "display": "stack",
    "access": "external",
    "depends": [
        {
            "key": "heartbleedserver",
            "required": true
        }
    ],
    "ports": [
        {
            "port": 80,
            "protocol": "http",
            "contextPath": "/"
        }
    ],
    "readinessProbe": {
        "type": "tcp",
        "path": "",
        "port": 80,
        "initialDelay": 30,
        "timeout": 500
    },
    "resourceLimits": {
        "cpuMax": 100,
        "cpuDefault": 100,
        "memMax": 100,
        "memDefault": 50
    },
	"authRequired": true,
    "info": "https://github.com/rkalyanapurdue/heartbleed/tree/master/heartbleed-hacker"
}
